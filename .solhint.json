{
    "extends": ["solhint:recommended"],
    "rules": {
        "prettier/prettier": "error",
        "avoid-throw": "off",
        "avoid-suicide": "error",
        "avoid-sha3": "warn",
        "contract-name-camelcase": "off",
        "compiler-fixed": "off",
        "two-lines-top-level-separator": "off"
    },
    "plugins": ["prettier"]
}
